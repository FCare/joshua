{
  "name": "Pipeline Text to Speech WebSocket",
  "description": "Pipeline WebSocket texte vers audio avec Chatterbox TTS",
  "category": "tts_ai",
  "version": "1.0.0",
  "capabilities": {
    "modalities": {
      "input": ["text"],
      "output": ["audio"],
      "processing": ["text-to-speech"]
    },
    "features": ["streaming", "french-voice"],
    "models": ["kyutai-tts"]
  },
  "step_instances": [
    {
      "instance_id": "websocket_server",
      "step_definition": "websocket_server",
      "config_overrides": {
        "port": 8767,
        "mode": "text_to_audio"
      }
    },
    {
      "instance_id": "kyutai_tts",
      "step_definition": "kyutai_tts",
      "config_overrides": {}
    }
  ],
  "connections": [
    {
      "from": "websocket_server",
      "to": "kyutai_tts"
    },
    {
      "from": "kyutai_tts",
      "to": "websocket_server"
    }
  ]
}